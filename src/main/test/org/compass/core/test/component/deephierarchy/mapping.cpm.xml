<!DOCTYPE compass-core-mapping PUBLIC
    "-//Compass/Compass Core Mapping DTD 1.0//EN"
    "http://www.opensymphony.com/compass/dtd/compass-core-mapping.dtd">

<compass-core-mapping package="org.compass.core.test.component.deephierarchy">

    <class name="A" alias="a">
        <id name="id" accessor="field" />
        <component name="bs" accessor="field" ref-alias="b" />
    </class>

    <class name="B" alias="b" root="false">
        <id name="id" accessor="field" />
        <component name="cs" accessor="field" ref-alias="c" />
    </class>

    <class name="C" alias="c" root="false">
        <id name="id" accessor="field" />
    </class>

</compass-core-mapping>
